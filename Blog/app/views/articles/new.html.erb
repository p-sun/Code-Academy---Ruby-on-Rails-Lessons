<h1>New Article</h1>

<!--
Pass the object :article to this form.
f is the form builder object
url: articles_path
-->
<%= form_for :article, url: articles_path do |f| %><p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>

    <p>
      <%= f.label :text %><br>
      <%= f.text_area :text %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>
